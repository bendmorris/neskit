function getbit(val: Uint, pos: Uint8): Bool {
    return (val & (1 << pos)) != 0;
}

function getbiti(val: Uint, pos: Uint8): Uint8 {
    return (val >> pos) & 1;
}

function reverseByte(b: Uint8): Uint8 {
    return ((b & 1) << 7) |
        (((b >> 1) & 1) << 6) |
        (((b >> 2) & 1) << 5) |
        (((b >> 3) & 1) << 4) |
        (((b >> 4) & 1) << 3) |
        (((b >> 5) & 1) << 2) |
        (((b >> 6) & 1) << 1) |
        (((b >> 7) & 1));
}
